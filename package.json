{
  "name": "world-wide-wishlist",
  "version": "1.0.1",
  "description": "A Chrome Extension that can save products from any shop website to one unified local wishlist",
  "main": "popupController.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "css-build": "node-sass --omit-source-map-url sass/mystyles.scss css/mystyles.css",
